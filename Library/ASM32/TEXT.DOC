        lea    ebx,wdata        ; EBX points to window coordinates
        lea    edi,buffer       ; point to buffer
        call   wsave
        .
        .
        .
; sometime later ...
; restore the window saved earlier
        lea    ebx,wdata        ; EBX points to window coordinates
        lea    edi,buffer       ; point to buffer
