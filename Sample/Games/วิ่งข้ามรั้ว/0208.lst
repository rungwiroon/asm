 01A2  33 C9		      C 	XOR	CX,CX
 01A4  BA 0016		      C 	MOV	DX,22D			; start at byte 22
 01A7  CD 21		      C 	INT	21H
			      C ;READ HEIGHT TO BUFFER AND SAVE TO BMP_HEIGHT
 01A9  B4 3F		      C 	MOV	AH,3FH
			      C 	;MOV	BX,BMP_HANDLE
 01AB  B9 0002		      C 	MOV	CX,2
 01AE  8D 16 0006 R	      C 	LEA	DX,BMP_BUFFER
 01B2  CD 21		      C 	INT	21H
 01B4  A1 0006 R	      C 	MOV	AX,WORD PTR BMP_BUFFER
 01B7  A3 0002 R	      C 	MOV	BMP_HEIGHT,AX
			      C 	
 01BA  C3		      C 	RET
			      C 
 01BB			      C READBMP	ENDP
			      C 	
			      C ;===========================================================
			      C ; READING IN THE COLOR PALETTE
			      C ;THIS CODE USE EFFECTIVE METHOD TO SET COLOR REGISTER
			      C ;BY NOT USE BIOS INTERRUPT BECAUSE IS VERY SLOW
			      C ;CHANGE TO USE OUT PORT DIRECTLY
			      C ;AND CALCULATE COLOR VALUE BY SHIFT BIT
			      C ;===========================================================
 01BB			      C SETCOLOR	PROC
			      C ;SEEK TO COLOR
 01BB  B8 4200		      C 	MOV	AX,4200H
 01BE  8B 1E 0004 R	      C 	MOV	BX,BMP_HANDLE
 01C2  33 C9		      C 	XOR	CX,CX
 01C4  BA 0036		      C 	MOV	DX,54D			; start at byte 54 
 01C7  CD 21		      C 	INT	21H
			      C 				
			      C ;READ 4 BYTES FROM BMP FILE TO BUFFER
 01C9  B4 3F		      C 	MOV	AH,3FH
			      C 	;MOV	BX,BMP_HANDLE
 01CB  B9 0400		      C 	MOV	CX,1024
 01CE  8D 16 0006 R	      C 	LEA	DX,BMP_BUFFER
 01D2  CD 21		      C 	INT	21H
			      C 		
 01D4  B0 00		      C 	MOV	AL,0				
 01D6  BA 03C8		      C 	MOV	DX,3C8H				;SAVE PORT NUMBER IN DX
 01D9  EE		      C 	OUT	DX,AL				;SEND START COLOR CODE
			      C 	
 01DA  8D 36 0006 R	      C 	LEA	SI,BMP_BUFFER
 01DE  BA 03C9		      C 	MOV	DX,3C9H
 01E1  B9 0100		      C 	MOV	CX,256
 01E4			      C COLOR_LOOP:
 01E4  8A 44 02		      C 	MOV	AL,[SI + 2]
 01E7  C0 E8 02		      C 	SHR	AL,2				;DIVIDE AL BY 2*2
 01EA  EE		      C 	OUT	DX,AL
 01EB  8A 44 01		      C 	MOV	AL,[SI + 1]
 01EE  C0 E8 02		      C 	SHR	AL,2				;DIVIDE AL BY 2*2
 01F1  EE		      C 	OUT	DX,AL
 01F2  8A 04		      C 	MOV	AL,[SI + 0]
 01F4  C0 E8 02		      C 	SHR	AL,2				;DIVIDE AL BY 2*2
 01F7  EE		      C 	OUT	DX,AL
 01F8  83 C6 04		      C 	ADD	SI,4
 01FB  E2 E7		      C 	LOOP	COLOR_LOOP		
			      C ; PALETTE READ END	
 01FD  C3		      C 	RET
			      C 	
 01FE			      C SETCOLOR	ENDP
			      C 
			      C ;===========================================================================
			      C ;CALCULATE POSITION FOR POINT TO START DRAW PICTURE 
			      C ;AND CHECK FOR BLANK PIXEL FOR DISPLAY PICTURE CORRECTLY
			      C ;===========================================================================
 01FE			      C SHOWBMP	PROC	
 01FE  B8 A000		      C 	MOV	AX, 0A000h		;START ADDRESS OF VGA BUFFER MEMORY
 0201  8E C0		      C 	MOV	ES, AX			;ES PONIT TO SCREEN SEGMENT
			      C 	
 0203  A1 0408 R	      C 	MOV	AX,BMP_Y
 0206  03 06 0002 R	      C 	ADD AX,BMP_HEIGHT
 020A  A3 040C R	      C 	MOV	BMP_Y2,AX			;DRAW PIC FROM BOTTOM
			      C 		
			      C ;COMPUTE BLANK PIXEL
 020D  A1 0000 R	      C 	MOV	AX,BMP_WIDTH
 0210  83 E0 03		      C 	AND	AX,03H			;AX MOD 4
 0213  83 F8 00		      C 	CMP	AX,0			;MOD 4 = 0?
 0216  75 08		      C 	JNE	SET_BMP_BLANK	;NO
 0218  C7 06 040E R 0000      C 	MOV	BMP_BLANK,0		;YES, SKIP WIDTH = 0
 021E  EB 0D		      C 	JMP	SHOW_LOOP
 0220			      C SET_BMP_BLANK:
 0220  A3 040E R	      C 	MOV	BMP_BLANK,AX	
 0223  B8 0004		      C 	MOV AX,4			
 0226  2B 06 040E R	      C 	SUB AX,BMP_BLANK	;HOW MANY BYTES TO SKIP?
 022A  A3 040E R	      C 	MOV BMP_BLANK,AX	;SAVE IT
			      C 
			      C ;===========================================================
			      C ;LOAD COLUMN OF PICTURE AND PUT TO SCREEN
			      C ;READ ALL PIXEL IN 1 LINE OF PIC FOR FAST DISPLAY
			      C ;===========================================================
 022D			      C SHOW_LOOP:
 022D  B4 3F		      C 	MOV	AH,3FH
 022F  8B 1E 0004 R	      C 	MOV	BX,BMP_HANDLE
 0233  8D 16 0006 R	      C 	LEA	DX,BMP_BUFFER
 0237  8B 0E 0000 R	      C 	MOV	CX,BMP_WIDTH	;READ 1 LINE OF PIC
 023B  CD 21		      C 	INT	21H
 023D  8D 36 0006 R	      C 	LEA	SI,BMP_BUFFER	;SI POINT TO BMP_BUFFER
			      C 	
			      C ;CALCULATE SCREEN OFFSET
 0241  B8 0140		      C 	MOV	AX, 320			;CALCULATE ROW
 0244  F7 26 040C R	      C 	MUL	BMP_Y2			;MUL AX BY Y,RESULT HIGH WORD IN DX,LOW WORD IN AX
 0248  03 06 0406 R	      C 	ADD	AX,BMP_X		;ADD X  CORDINATE
 024C  8B F8		      C 	MOV	DI,AX			;DI POINT TO SCREEN OFFSET
			      C 	
 024E			      C BMP_TRANSPARENT:	
 024E  80 3E 0411 R 02	      C 	CMP	BMP_MODE,2
 0253  75 05		      C 	JNE	BMP_FAST
 0255  E8 004F		      C 	CALL	PUTPIXEL_TRAN
 0258  EB 11		      C 	JMP	NEXT_LINE
			      C 	
 025A			      C BMP_FAST:	
 025A  83 3E 040E R 00	      C 	CMP	BMP_BLANK,0
 025F  75 05		      C 	JNE	BMP_NORM
 0261  E8 0037		      C 	CALL	PUTPIXEL_FAST
 0264  EB 17		      C 	JMP	NO_BLANK
			      C 	
 0266			      C BMP_NORM:
 0266  E8 002A		      C 	CALL	PUTPIXEL_NORM
 0269  EB 00		      C 	JMP	NEXT_LINE
			      C 
 026B			      C NEXT_LINE:
 026B  83 3E 040E R 00	      C 	CMP	BMP_BLANK,0
 0270  74 0B		      C 	JE	NO_BLANK
 0272  B8 4201		      C 	MOV AX,4201H
 0275  33 C9		      C 	XOR CX,CX
 0277  8B 16 040E R	      C 	MOV DX,BMP_BLANK
 027B  CD 21		      C 	INT	21H
 027D			      C NO_BLANK:	
 027D  FF 0E 040C R	      C 	DEC	BMP_Y2			;DISPLAY UPPER LINE	
 0281  A1 040C R	      C 	MOV	AX,BMP_Y2
 0284  3B 06 0408 R	      C 	CMP	AX,BMP_Y		;TOP LINE?
 0288  77 A3		      C 	JA	SHOW_LOOP		;NO SHOW NEXT LINE
			      C 	
			      C ;====================================================
			      C ;DISPLAY PICTURE COMPLETE!
			      C ;====================================================
			      C ;CLOSE BMP FILE
 028A  B4 3E		      C 	MOV	AH,3EH
 028C  8B 1E 0004 R	      C 	MOV	BX,BMP_HANDLE
 0290  CD 21		      C 	INT	21H
			      C 	
 0292  C3		      C 	RET
			      C 	
 0293			      C SHOWBMP	ENDP
			      C 
			      C ;==========================================================================================
			      C ;Normal putpixel to scrren by load 1 byte from BMP_BUFFER to VGA segment
			      C ;==========================================================================================
 0293			      C PUTPIXEL_NORM	PROC
 0293  FC		      C 	CLD					;CLEAR DIRECTION FLAG FOR SET TO INC DI,SI
 0294  8B 0E 0000 R	      C 	MOV	CX,BMP_WIDTH
 0298  F3/ A4		      C 	REP MOVSB			;REPEAT MOV ES:[DI],DS:[SI] AND INC SI,DI
			      C 						;DISPLAY 1 LINE COMPLETE
 029A  C3		      C 	RET
			      C 	
 029B			      C PUTPIXEL_NORM	ENDP
			      C 
			      C ;==========================================================================================
			      C ;Fast putpixel to scrren by load 1 Double word from BMP_BUFFER to VGA segment
			      C ;==========================================================================================
 029B			      C PUTPIXEL_FAST	PROC
 029B  FC		      C 	CLD
 029C  8B 0E 0000 R	      C 	MOV	CX,BMP_WIDTH
 02A0  C1 E9 02		      C 	SHR	CX,2				;DIVIDE CX BY 4
 02A3  F3/ 66| A5	      C 	REP	MOVSD				;MOV ES:[DI],AL AND INC DI
			      C 							;DISPLAY 1 LINE COMPLETE
 02A6  C3		      C 	RET
			      C 	
 02A7			      C PUTPIXEL_FAST	ENDP
			      C 
			      C ;======================================================================================================
			      C ;Fast putpixel to scrren by load 1 Byte from BMP_BUFFER and check transparent for skip put to VGA segment
			      C ;======================================================================================================
 02A7			      C PUTPIXEL_TRAN	PROC
 02A7  FC		      C 	CLD
 02A8  8B 0E 0000 R	      C 	MOV	CX,BMP_WIDTH	;SET CX FOR LOOP BMP_BUFFER
			      C 			
 02AC			      C PUT_LOOP:
 02AC  AC		      C 	LODSB				;MOV AL,BYTE PTR DS:[SI] AND INC SI
 02AD  3A 06 0410 R	      C 	CMP	AL,TRANSPARENT	;IT TRANSPARENT COLOR?
 02B1  75 03		      C 	JNE	PUTPIXEL_T		;NO,PUTPIXEL!
 02B3  47		      C 	INC	DI				;IF SKIP COLOR MANUAL INC DI
 02B4  EB 01		      C 	JMP	SKIP_COLOR
			      C 	
 02B6			      C PUTPIXEL_T:
 02B6  AA		      C 	STOSB				;MOV ES:[DI],AL AND INC DI
			      C 
 02B7			      C SKIP_COLOR:
 02B7  E2 F3		      C 	LOOP	PUT_LOOP	;NO SHOW NEXT PIXEL OF LINE
			      C 						;DISPLAY 1 LINE COMPLETE
 02B9  C3		      C 	RET
			      C 	
 02BA			      C PUTPIXEL_TRAN	ENDP
			      C 
