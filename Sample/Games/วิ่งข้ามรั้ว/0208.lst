 02BA			      C 	SHOWMENU	PROC		;MENU
			      C 	
			      C 	;PUSHA
			      C 	
 02BA  C7 06 0406 R 0000      C 	MOV	BMP_X,0
 02C0  C7 06 0408 R 0000      C 	MOV	BMP_Y,0
			      C 	PASTEBMP	0,0,MENUPLAY
 02C6  6A 00		     1C 	PUSH	0
 02C8  8F 06 0406 R	     1C 	POP	BMP_X
 02CC  6A 00		     1C 	PUSH	0
 02CE  8F 06 0408 R	     1C 	POP	BMP_Y
 02D2  8D 36 0415 R	     1C 	LEA		SI,MENUPLAY
 02D6  C6 06 0411 R 00	     1C 	MOV		BMP_MODE,0
 02DB  E8 FE4D		     1C 	CALL	LOADBMP
			      C 	
 02DE			      C MENU_LOOP:
 02DE  B4 08		      C 	MOV	AH,08			;WAITING FOR KEY
 02E0  CD 21		      C 	INT	21H		
 02E2  3C 48		      C 	CMP	AL,72			;CHECK KEY UP ARROW
 02E4  74 0C		      C 	JE	UP_ARROW
 02E6  3C 50		      C 	CMP	AL,80			;CHECK KEY DOWN ARROW
 02E8  74 2A		      C 	JE	DOWN_ARROW
 02EA  3C 0D		      C 	CMP	AL,13			;CHECK KEY ENTER
 02EC  0F 84 00D8	      C 	JE	ENTER_MENU
 02F0  EB EC		      C 	JMP	MENU_LOOP		; LOOP MENU_LOOP
			      C 	
 02F2			      C UP_ARROW:				;LOOP	UP ARROW
 02F2  FE 0E 05AB R	      C 	DEC	CHOICE			;DECREASE CHOICE
 02F6  80 3E 05AB R 01	      C 	CMP	CHOICE,1		;CHECK CHOICE
 02FB  7E 37		      C 	JLE	MENU_PLAY
 02FD  80 3E 05AB R 02	      C 	CMP	CHOICE,2
 0302  74 56		      C 	JE	MENU_HIGH
 0304  80 3E 05AB R 03	      C 	CMP	CHOICE,3
 0309  74 71		      C 	JE	MENU_HOW
 030B  80 3E 05AB R 04	      C 	CMP	CHOICE,4
 0310  0F 84 008A	      C 	JE	MENU_EXIT
			      C 	
 0314			      C DOWN_ARROW:				;LOOP DOWN ARROW
 0314  FE 06 05AB R	      C 	INC	CHOICE			;INCREASE CHOICE
 0318  80 3E 05AB R 01	      C 	CMP	CHOICE,1		;CHECK CHOICE
 031D  74 15		      C 	JE	MENU_PLAY
 031F  80 3E 05AB R 02	      C 	CMP	CHOICE,2
 0324  74 34		      C 	JE	MENU_HIGH
 0326  80 3E 05AB R 03	      C 	CMP	CHOICE,3
 032B  74 4F		      C 	JE	MENU_HOW
 032D  80 3E 05AB R 04	      C 	CMP	CHOICE,4
 0332  7D 6A		      C 	JGE	MENU_EXIT
			      C 	
 0334			      C MENU_PLAY:
 0334  80 3E 05AB R 01	      C 	CMP	CHOICE,1		;CHOICE = 1 DISPLAY MENUPLAY
 0339  7C 1A		      C 	JL	CHOICE_LESS
			      C 	PASTEBMP	0,0,MENUPLAY
 033B  6A 00		     1C 	PUSH	0
 033D  8F 06 0406 R	     1C 	POP	BMP_X
 0341  6A 00		     1C 	PUSH	0
 0343  8F 06 0408 R	     1C 	POP	BMP_Y
 0347  8D 36 0415 R	     1C 	LEA		SI,MENUPLAY
 034B  C6 06 0411 R 00	     1C 	MOV		BMP_MODE,0
 0350  E8 FDD8		     1C 	CALL	LOADBMP
 0353  EB 89		      C 	JMP	MENU_LOOP
			      C 	
 0355			      C CHOICE_LESS:
 0355  C6 06 05AB R 04	      C 	MOV	CHOICE,4
			      C 	
 035A			      C MENU_HIGH:
 035A  80 3E 05AB R 02	      C 	CMP	CHOICE,2		;CHOICE = 2 DISPLAY MENUHIGH
 035F  75 1B		      C 	JNE	MENU_HOW
			      C 	PASTEBMP	0,0,MENUHIGH
 0361  6A 00		     1C 	PUSH	0
 0363  8F 06 0406 R	     1C 	POP	BMP_X
 0367  6A 00		     1C 	PUSH	0
 0369  8F 06 0408 R	     1C 	POP	BMP_Y
 036D  8D 36 0426 R	     1C 	LEA		SI,MENUHIGH
 0371  C6 06 0411 R 00	     1C 	MOV		BMP_MODE,0
 0376  E8 FDB2		     1C 	CALL	LOADBMP
 0379  E9 FF62		      C 	JMP	MENU_LOOP	
			      C 	
 037C			      C MENU_HOW:
 037C  80 3E 05AB R 03	      C 	CMP	CHOICE,3		;CHOICE = 3 DISPLAY MENUHOW
 0381  75 1B		      C 	JNE	MENU_EXIT
			      C 	PASTEBMP	0,0,MENUHOW
 0383  6A 00		     1C 	PUSH	0
 0385  8F 06 0406 R	     1C 	POP	BMP_X
 0389  6A 00		     1C 	PUSH	0
 038B  8F 06 0408 R	     1C 	POP	BMP_Y
 038F  8D 36 0437 R	     1C 	LEA		SI,MENUHOW
 0393  C6 06 0411 R 00	     1C 	MOV		BMP_MODE,0
 0398  E8 FD90		     1C 	CALL	LOADBMP
 039B  E9 FF40		      C 	JMP	MENU_LOOP
			      C 	
 039E			      C MENU_EXIT:
 039E  80 3E 05AB R 04	      C 	CMP	CHOICE,4
 03A3  7F 1B		      C 	JG	CHOICE_MORE
			      C 	PASTEBMP	0,0,MENUEXIT
 03A5  6A 00		     1C 	PUSH	0
 03A7  8F 06 0406 R	     1C 	POP	BMP_X
 03AB  6A 00		     1C 	PUSH	0
 03AD  8F 06 0408 R	     1C 	POP	BMP_Y
 03B1  8D 36 0447 R	     1C 	LEA		SI,MENUEXIT
 03B5  C6 06 0411 R 00	     1C 	MOV		BMP_MODE,0
 03BA  E8 FD6E		     1C 	CALL	LOADBMP
 03BD  E9 FF1E		      C 	JMP	MENU_LOOP
			      C 	
 03C0			      C CHOICE_MORE:
 03C0  C6 06 05AB R 01	      C 	MOV	CHOICE,1
 03C5  E9 FF6C		      C 	JMP	MENU_PLAY
			      C 	
 03C8			      C ENTER_MENU:		;ENTER FOR RETURN
			      C 	;POPA
 03C8  C3		      C 	RET
